 <div class="row text-center">
  <div class="col-md-6">
    <h3>Delivery Information</h4>
    <hr>
    <h4 id='address-confirm'>Address</h5>
    <h4 id='phone-confirm'>Phone Number</h5>
  </div>
   <div class="col-md-6">
    <h3>Order Information</h4>
    <hr>
    {{#each cart}}
    <h4>{{name}}:<small>${{price}} x {{quantity}} = {{itemTotalAmount}}</small></h4>
    {{/each}}
    <hr>
    <h4>Total: {{cart.total}}</h4>
    <h6>(Tax and delivery fees included)<h5>
  </div>
</div>
