			<script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
			<script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
	<![endif]-->
</head>

<body id="page-top" class="index">

		<!-- Navigation -->
		<nav class="navbar navbar-default navbar-fixed-top">
				<div class="container">
						<!-- Brand and toggle get grouped for better mobile display -->
						<div class="navbar-header page-scroll">
								<button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1">
										<span class="sr-only">Toggle navigation</span>
										<span class="icon-bar"></span>
										<span class="icon-bar"></span>
										<span class="icon-bar"></span>
								</button>
								<%=link_to "HACKMAI", root_path, class: "navbar-brand"%>
								<h1 class="hidden">Hackmai</h1>
								<!-- Flash message -->
								<% flash.each do |name, msg| %>
								 <% if msg.is_a?(String) %>
									 <%= content_tag :div, msg, :class => "flash_#{name}" %>
								 <% end %>
								<% end %>
						</div>

						<!-- Collect the nav links, forms, and other content for toggling -->
						<div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
								<ul class="nav navbar-nav navbar-right">
										<li class="hidden">
												<a href="#page-top"></a>
										</li>
										<li>
												<a href="javascript:void(0)" id="about-link">About us</a>
										</li>
										<li>
												<a href="javascript:void(0)" id="order-link">Order</a>
										</li>
								 		<li class="dropdown">
								 				<% if user_signed_in? %>
								        <a href="#" data-toggle="dropdown" class="dropdown-toggle">
								        <%= current_user.username.present? ? current_user.username : current_user.email%>
												<b class="caret"></b></a>
								        <ul class="dropdown-menu">
								            <li><%= link_to 'Edit profile', edit_user_registration_path, id: "drop-down-list"%></li>
								            <li><%= link_to "Logout", destroy_user_session_path, method: :delete, id: "drop-down-list" %></li>
								        <% else %>
								        <a href="#" data-toggle="dropdown" class="dropdown-toggle">Sign-up/Login <b class="caret"></b></a>
								        	<ul class="dropdown-menu">
									        	<li><%= link_to "Login", new_user_session_path, id: "drop-down-list"%></li>
										        <li><%= link_to "Sign up", new_user_registration_path, id: "drop-down-list"%></li>
												<% end %>
								        </ul>
								    </li>
								</ul>
						</div>
						<!-- /.navbar-collapse -->
				</div>
				<!-- /.container-fluid -->
		</nav>

<%= yield %>

